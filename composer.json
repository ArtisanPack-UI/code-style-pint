{
  "name": "artisanpack-ui/code-style-pint",
  "description": "Laravel Pint preset for ArtisanPack UI code standards",
  "type": "library",
  "license": "GPL-3.0-or-later",
  "version": "1.0.0",
  "keywords": [
    "laravel",
    "pint",
    "code-style",
    "php-cs-fixer",
    "artisanpack"
  ],
  "autoload": {
    "psr-4": {
      "ArtisanPackUI\\CodeStylePint\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Jacob Martella",
      "email": "me@jacobmartella.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "laravel/pint": "^1.0",
    "orchestra/testbench": "^9.0|^10.0",
    "pestphp/pest": "^3.8",
    "pestphp/pest-plugin-laravel": "^3.1"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ArtisanPackUI\\CodeStylePint\\CodeStylePintServiceProvider"
      ]
    }
  }
}
